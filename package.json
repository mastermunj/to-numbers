{
  "name": "to-numbers",
  "version": "0.0.1",
  "description": "Converts words (including decimal points) into numbers & currency. The reverse of to-words.",
  "keywords": [
    "to numbers",
    "words to numbers",
    "words-to-numbers",
    "words to numbers currency",
    "international words to numbers",
    "international words to numbers currency",
    "convert-words",
    "i18n",
    "converter",
    "number",
    "word",
    "currency",
    "parse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mastermunj/to-numbers.git"
  },
  "license": "MIT",
  "author": "Munjal Dhamecha",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/ToNumbers.d.ts",
      "import": "./dist/esm/ToNumbers.js",
      "require": "./dist/cjs/ToNumbers.js"
    },
    "./*": {
      "types": "./dist/types/locales/*.d.ts",
      "import": "./dist/esm/locales/*.js",
      "require": "./dist/cjs/locales/*.js"
    },
    "./types": {
      "types": "./dist/types/types.d.ts",
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.js"
    },
    "./core": {
      "types": "./dist/types/ToNumbersCore.d.ts",
      "import": "./dist/esm/ToNumbersCore.js",
      "require": "./dist/cjs/ToNumbersCore.js"
    }
  },
  "main": "dist/cjs/ToNumbers.js",
  "jsdelivr": "dist/umd/to-numbers.min.js",
  "unpkg": "dist/umd/to-numbers.min.js",
  "module": "dist/esm/ToNumbers.js",
  "browser": "dist/umd/to-numbers.min.js",
  "types": "dist/types/ToNumbers.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/locales/*"
      ],
      "types": [
        "dist/types/types"
      ],
      "core": [
        "dist/types/ToNumbersCore"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "bench": "vitest bench",
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:pkgjson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:pkgjson": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:types": "tsc -p tsconfig.json",
    "build:umd": "node --experimental-strip-types scripts/build-umd.ts",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf dist coverage",
    "commit": "git-cz",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "release:mock": "npm run release -- --dry-run",
    "test": "vitest",
    "test:watch": "npm run test -- --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "commit-and-tag-version": "^12.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "lodash": "^4.17.21",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "sort-package-json": "^3.6.0",
    "source-map-support": "^0.5.21",
    "to-words": "^5.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
